{
  "name": "file-converter",
  "appname": "file-converter",
  "version": "0.0.1",
  "description": "A client-side app that lets you convert between documents written in various markup languages.",
  "dependencies": {
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babelify": "^6.0.2",
    "browserify": "^10.1.3",
    "watchify": "^3.2.1"
  },
  "scripts": {
    "build": "browserify app.js -t babelify --outfile bundle.js",
    "watch": "watchify app.js -t babelify --outfile bundle.js -dv"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/fraserxu/file-converter.git"
  },
  "author": "Fraser Xu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fraserxu/file-converter/issues"
  }
}
